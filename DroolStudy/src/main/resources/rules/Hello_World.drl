package demo
import com.droolstuday.example.model.Account
rule "helloworld"
  when
  $account : Account( balance < 100 )
  then
  System.out.println("1");
end

rule "helloworld1"
  when
  $account : Account( balance < 101 )
  then
  System.out.println("2");
end


rule "helloworld2"
  when
  $account : Account( balance > 100 )
  then
  System.out.println("OK");
end