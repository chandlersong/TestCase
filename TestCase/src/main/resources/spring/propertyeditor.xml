<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="DependsOnExoticType"
		class="org.springexample.propertyeditor.DependsOnExoticType">
		<property name="type" value="Success"/>
	</bean>
	
	<bean class="org.springframework.beans.factory.config.CustomEditorConfigurer">
		<property name="customEditors">
			<map>
				<entry key="org.springexample.propertyeditor.ExoticType" value="org.springexample.propertyeditor.ExoticTypeEditor"/>
			</map>
		</property>
	</bean>
</beans>
